{
  "customTemplates": [
    {
      "name": "page-no-title",
      "postTypes": ["page"],
      "title": "Page No Title"
    }
  ],
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "gradients": [
        {
          "gradient": "linear-gradient(135deg,#144D29 0%,#E6F5EC 100%)",
          "name": "Color 1",
          "slug": "custom-color-1"
        }
      ],
      "palette": [
        {
          "color": "#ffffff",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#000000",
          "name": "Contrast",
          "slug": "contrast"
        },
        {
          "color": "#144D29",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#e4f5ea",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#F6F6F6",
          "name": "Tertiary",
          "slug": "tertiary"
        },
        {
          "color": "#E6F5EC",
          "name": "Pale",
          "slug": "pale"
        },
        {
          "color": "#F7F9F7",
          "name": "light",
          "slug": "light"
        }
      ]
    },
    "layout": {
      "allowCustomContentAndWideSize": true,
      "allowEditing": true,
      "contentSize": "60rem",
      "wideSize": "75rem"
    },
    "spacing": {
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "Tiny",
          "size": "0.5rem",
          "slug": "20"
        },
        {
          "name": "X-Small",
          "size": "1rem",
          "slug": "30"
        },
        {
          "name": "Small",
          "size": "1.5rem",
          "slug": "40"
        },
        {
          "name": "Regular",
          "size": "clamp(2rem, 5vw, 3rem)",
          "slug": "50"
        },
        {
          "name": "Large",
          "size": "clamp(2rem, 7vw, 4rem)",
          "slug": "60"
        },
        {
          "name": "X-Large",
          "size": "clamp(3rem, 7vw, 5rem)",
          "slug": "70"
        },
        {
          "name": "XX-Large",
          "size": "clamp(4rem, 10vw, 8rem)",
          "slug": "80"
        }
      ],
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "defaultFontSizes": false,
      "fluid": true,

      "fontFamilies": [
        {
          "fontFamily": "Montserrat, sans-serif",
          "name": "Montserrat",
          "slug": "montserrat"
        }
      ],
      "fontSizes": [
        {
          "fluid": false,
          "name": "Small",
          "size": "0.875rem",
          "slug": "small"
        },
        {
          "fluid": {
            "max": "1.125rem",
            "min": "1rem"
          },
          "name": "Medium",
          "size": "1rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "max": "1.375rem",
            "min": "1.125rem"
          },
          "name": "Large",
          "size": "1.38rem",
          "slug": "large"
        },
        {
          "fluid": {
            "max": "2rem",
            "min": "1.75rem"
          },
          "name": "Extra Large",
          "size": "1.75rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "max": "3rem",
            "min": "2.15rem"
          },
          "name": "Extra Extra Large",
          "size": "2.15rem",
          "slug": "xx-large"
        }
      ],
      "fontWeight": true,
      "writingMode": true
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "blocks": {
      "core/media-text": {
        "color": {
          "gradient": "linear-gradient(90deg,rgb(6,147,227) 0%,rgb(155,81,224) 75%)"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--30)",
            "top": "var(--wp--preset--spacing--30)"
          },
          "padding": {
            "bottom": "0",
            "left": "0",
            "right": "0",
            "top": "0"
          }
        }
      },
      "core/paragraph": {
        "typography": {
          "fontWeight": "400",
          "lineHeight": "1.6"
        }
      },
      "domki-section/domki-section": {
        "background": {
          "backgroundImage": "none",
          "backgroundSize": "auto"
        },
        "elements": {
          "heading": {
            "color": {
              "text": "var(--wp--preset--color--contrast)"
            }
          }
        }
      }
    },
    "color": {
      "text": "var(--wp--preset--color--contrast)"
    },
    "elements": {
      "h1": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--30)",
            "top": "var(--wp--preset--spacing--30)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)"
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "lineHeight": "1.3"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontStyle": "normal",
          "fontWeight": "400",
          "lineHeight": "1.3",
          "textDecoration": "none"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--montserrat)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "fontStyle": "normal",
      "fontWeight": "400"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ],
  "version": 3,
  "$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}
